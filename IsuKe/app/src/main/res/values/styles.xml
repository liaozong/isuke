<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
    </style>

    <style name="CustomTheme" parent="DefaultCityPickerTheme">
        <item name="cpCancelTextColor">@color/colorPrimary</item>
        <item name="cpSearchCursorDrawable">@color/colorPrimary</item>
        <item name="cpIndexBarNormalTextColor">@color/colorPrimary</item>
        <item name="cpIndexBarSelectedTextColor">@color/colorPrimary</item>
        <item name="cpOverlayBackground">@color/colorPrimary</item>
    </style>

    <!--progroessbar-->
    <style name="NumberProgressBar_Default">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>

        <item name="max">100</item>
        <item name="progress">0</item>

        <item name="progress_unreached_color">#CCCCCC</item>
        <item name="progress_reached_color">#3498DB</item>

        <item name="progress_text_size">10sp</item>
        <item name="progress_text_color">#3498DB</item>

        <item name="progress_reached_bar_height">1.5dp</item>
        <item name="progress_unreached_bar_height">0.75dp</item>
    </style>
    <!--loginEdit-->
    <style name="LoginEdit">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@null</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/heise</item>
        <item name="android:textColorHint">@color/danhui</item>
        <item name="android:textSize">14sp</item>
    </style>

    <!--个人信息页面item中的icon-->
    <style name="MineItemIcon">
        <item name="android:layout_width">@dimen/dpDimen20</item>
        <item name="android:layout_height">@dimen/dpDimen20</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/dpDimen15</item>
    </style>

    <!--个人信息分割线-->
    <style name="MineItemLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dpDimen01</item>
        <item name="android:background">@color/huise</item>
        <item name="android:layout_marginLeft">@dimen/dpDimen45</item>
    </style>
    <!--个人信息条目中的字-->
    <style name="MineItemWord">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/dpDimen15</item>
        <item name="android:textColor">@color/text_color_normal</item>
        <item name="android:textSize">@dimen/spDimen15</item>
    </style>


    <!--个人信息条目中的箭头-->
    <style name="MineIconArrow">
        <item name="android:layout_width">@dimen/dpDimen25</item>
        <item name="android:layout_height">@dimen/dpDimen25</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:src">@mipmap/ic_arrow</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">@dimen/dpDimen15</item>
    </style>
    <!--普通的Linear-->
    <style name="Linearlayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>
    <!--个人信息条目高48-->
    <style name="Item48">
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/baise</item>
        <item name="android:layout_height">@dimen/dpDimen48</item>
    </style>


    <!--白色的Linear-->
    <style name="Linearlayout_baise">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/baise</item>
        <item name="android:orientation">vertical</item>
    </style>
    <!--条目中右边显示的文字-->
    <style name="right_word">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">@dimen/dpDimen15</item>
    </style>

    <!--登陆按钮-->

    <style name="login_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:background">@drawable/login_selector_btn</item>
        <item name="android:textColor">@color/baise</item>
        <item name="android:textSize">16sp</item>
    </style>

    <!--app图标-->
    <style name="image_appicon">
        <item name="android:layout_width">@dimen/dpDimen120</item>
        <item name="android:layout_height">@dimen/dpDimen120</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:src">@mipmap/ic_sukelogo01</item>
    </style>
    <!--app图标-->
    <style name="commom_back_image">
        <item name="android:layout_width">@dimen/dpDimen20</item>
        <item name="android:layout_height">@dimen/dpDimen20</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/dpDimen15</item>
        <item name="android:background">@mipmap/ic_backblack</item>
    </style>
    <!--头添加-->
    <style name="titleicon">
        <item name="android:layout_width">@dimen/dpDimen25</item>
        <item name="android:layout_height">@dimen/dpDimen25</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">@dimen/dpDimen15</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:src">@mipmap/ic_addhome</item>
    </style>
    <style name="titleright">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">@dimen/dpDimen15</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <!--popwindow style-->
    <style name="myanimation">
        <!--窗体进入动画-->
        <item name="android:windowEnterAnimation">@anim/window_pop_anim</item>
        <!--窗体退出动画-->
        <item name="android:windowExitAnimation">@anim/popup_window_exit</item>
    </style>

    <!--橙色按钮风格-->
    <style name="orgcolor_btnstyle">
        <item name="android:layout_width">@dimen/dpDimen210</item>
        <item name="android:layout_height">@dimen/dpDimen48</item>
        <item name="android:background">@drawable/login_selector_btn</item>
        <item name="android:textColor">@color/baise</item>
    </style>

    <style name="AboutUsItemTV">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/text_color_normal</item>
        <item name="android:textSize">@dimen/spDimen16</item>
    </style>

    <style name="AboutUsItem">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dpDimen48</item>
        <item name="android:background">@color/baise</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">@dimen/dpDimen20</item>
    </style>

    <style name="LL_UserInfo">
        <item name="android:background">@android:color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">@dimen/dpDimen15</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="TV_UserInfoTip">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">@dimen/spDimen15</item>
    </style>


</resources>
