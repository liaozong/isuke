<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/org_word"
        android:orientation="vertical">

        <View
            android:id="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dpDimen01" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dpDimen48"
            android:layout_marginTop="@dimen/dpDimen20">

            <RelativeLayout
                android:id="@+id/title_back"
                android:layout_width="@dimen/dpDimen80"
                android:layout_height="match_parent">

                <ImageView
                    style="@style/commom_back_image"
                    android:background="@mipmap/ic_back" />
            </RelativeLayout>

            <TextView
                android:id="@+id/title_theme"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:textColor="@color/baise"
                android:textSize="@dimen/spDimen16" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/isuke_background">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">


                    <RelativeLayout
                        android:id="@+id/set_remark"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dpDimen48"
                        android:background="@color/baise">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/dpDimen15"
                            android:text="@string/set_remark" />

                        <TextView
                            android:id="@+id/set_name"
                            android:layout_width="@dimen/dpDimen200"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:gravity="right"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dpDimen30" />

                        <ImageView
                            android:layout_width="@dimen/dpDimen25"
                            android:layout_height="@dimen/dpDimen25"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dpDimen10"
                            android:background="@mipmap/ic_arrow" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/shared_user"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dpDimen48"
                        android:layout_marginTop="@dimen/dpDimen10"
                        android:background="@color/baise">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/dpDimen15"
                            android:text="@string/shared_user" />

                        <TextView
                            android:id="@+id/shared_user_num"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dpDimen30" />

                        <ImageView
                            android:layout_width="@dimen/dpDimen25"
                            android:layout_height="@dimen/dpDimen25"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dpDimen10"
                            android:background="@mipmap/ic_arrow" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/time_opening"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dpDimen48"
                        android:layout_marginTop="@dimen/dpDimen10"
                        android:background="@color/baise">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/dpDimen15"
                            android:text="@string/timing_start" />

                        <ImageView
                            android:layout_width="@dimen/dpDimen25"
                            android:layout_height="@dimen/dpDimen25"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dpDimen10"
                            android:background="@mipmap/ic_arrow" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dpDimen01"
                            android:layout_alignParentBottom="true"
                            android:layout_marginLeft="@dimen/dpDimen15"
                            android:background="@color/huise" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/power_total"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dpDimen48"
                        android:background="@color/baise">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/dpDimen15"
                            android:text="@string/PowerTutor" />

                        <ImageView
                            android:layout_width="@dimen/dpDimen25"
                            android:layout_height="@dimen/dpDimen25"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dpDimen10"
                            android:background="@mipmap/ic_arrow" />

                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/device_update"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dpDimen48"
                        android:layout_marginTop="@dimen/dpDimen10"
                        android:background="@color/baise">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/dpDimen15"
                            android:text="@string/device_update" />

                        <TextView
                            android:id="@+id/version_code"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dpDimen30" />

                        <ImageView
                            android:layout_width="@dimen/dpDimen25"
                            android:layout_height="@dimen/dpDimen25"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dpDimen10"
                            android:background="@mipmap/ic_arrow" />

                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/device_advice"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dpDimen48"
                        android:layout_marginTop="@dimen/dpDimen10"
                        android:background="@color/baise">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/dpDimen15"
                            android:text="@string/adviceor" />

                        <ImageView
                            android:layout_width="@dimen/dpDimen25"
                            android:layout_height="@dimen/dpDimen25"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dpDimen10"
                            android:background="@mipmap/ic_arrow" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/delete_device"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dpDimen48"
                        android:layout_marginTop="@dimen/dpDimen10"
                        android:background="@color/baise">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/dpDimen15"
                            android:text="@string/delete_device"
                            android:textColor="@color/org_login_word" />

                    </RelativeLayout>
                </LinearLayout>
            </ScrollView>

        </RelativeLayout>

    </LinearLayout>
</layout>