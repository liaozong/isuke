<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dpDimen260"
        android:background="@drawable/popbackground_shape"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dpDimen38">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dpDimen15" />

            <ImageView
                android:id="@+id/cancle"
                android:layout_width="@dimen/dpDimen25"
                android:layout_height="@dimen/dpDimen25"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/dpDimen15"
                android:background="@mipmap/ic_off01" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/wifi_icon"
                android:layout_width="@dimen/dpDimen60"
                android:layout_height="@dimen/dpDimen60"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/dpDimen30"
                android:background="@mipmap/ic_wifi" />

            <TextView
                android:id="@+id/nowifi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/wifi_icon"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/dpDimen20"
                android:text="@string/phone_nowifi"
                android:textSize="@dimen/spDimen16" />

            <TextView
                android:id="@+id/go_conn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/nowifi"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/dpDimen20"
                android:text="@string/go_conn"
                android:textColor="@color/org_login_word" />

        </RelativeLayout>

    </LinearLayout>

</RelativeLayout>